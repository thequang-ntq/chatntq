<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="chatntq.APP.Components.ImageButton.ImageButtonComp">
    <ImageButton
        x:Name="ImageButtonComponent"
        Source="{Binding Source, Source={RelativeSource AncestorType={x:Type local:ImageButtonComp}}}"
        Clicked="ImageButtonComponent_Clicked"
        BackgroundColor="Transparent"
        VerticalOptions="{Binding VerticalOptions, Source={RelativeSource AncestorType={x:Type local:ImageButtonComp}}}"
        HorizontalOptions="{Binding HorizontalOptions, Source={RelativeSource AncestorType={x:Type local:ImamgeButtonComp}}}"   
    />
</ContentView>
